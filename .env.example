# FastAPI服务配置示例
# 复制此文件为.env并根据需要修改配置

# 应用配置
APP_NAME=时间序列模型传递函数分析器 API
APP_VERSION=0.1.0
DEBUG=false

# 服务器配置
HOST=0.0.0.0
PORT=8000
RELOAD=true

# 日志配置
LOG_LEVEL=info

# CORS配置（生产环境中应该限制具体域名）
CORS_ORIGINS=["*"]
CORS_ALLOW_CREDENTIALS=true

# 分析配置
DEFAULT_PRECISION=4
MAX_LAG_LIMIT=100
MAX_FREQUENCY_POINTS=1000
